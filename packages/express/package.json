{
  "name": "@schmock/express",
  "version": "0.1.0",
  "description": "Express adapter for Schmock mock API generator",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build:lib && bun build:types",
    "build:lib": "bun build --minify --outdir=dist src/index.ts",
    "build:types": "tsc -p tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "biome check src/**/*.ts",
    "lint:fix": "biome check --write --unsafe src/**/*.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@schmock/builder": "^0.1.0",
    "express": "^4.18.0"
  },
  "keywords": [
    "mock",
    "api",
    "express",
    "testing",
    "development"
  ],
  "author": "Khalic Lab",
  "license": "MIT"
}